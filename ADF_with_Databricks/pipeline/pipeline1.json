{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Extract_customer_data_Simple_JSON_using_SparkSQL",
				"type": "DatabricksNotebook",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"baseParameters": {
						"p_data_source ": {
							"value": "@variables('v_data_source')",
							"type": "Expression"
						},
						"p_file_date": {
							"value": "@formatDateTime(pipeline().parameters.p_window_end_date,'yyyy-MM-dd')",
							"type": "Expression"
						},
						"configJson": {
							"value": "@pipeline().parameters.configJson",
							"type": "Expression"
						}
					}
				}
			}
		],
		"parameters": {
			"p_window_end_date": {
				"type": "string"
			},
			"configJson": {
				"type": "string",
				"defaultValue": "\"{\\\"source_path\\\": \\\"/mnt/data/input/\\\", \\\"target_table\\\": \\\"sales_table\\\", \\\"env\\\": \\\"dev\\\", \\\"max_records\\\": 1000}\""
			}
		},
		"variables": {
			"v_data_source": {
				"type": "String",
				"defaultValue": "TCS"
			}
		},
		"annotations": []
	}
}